<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bx.kh.khprovider.dao.CustomerinfoDao">

      <insert id="addinfo" parameterType="Customerinfo">
           insert into customerinfo(userId,userName,birthday,phone,address,
             sex,Email,IdentityCard,createTime,updateTime,type)
             values (#{userId},#{userName},#{birthday},#{phone},#{address},
             #{sex},#{Email},#{IdentityCard},now(),now(),#{type})

      </insert>

</mapper>
